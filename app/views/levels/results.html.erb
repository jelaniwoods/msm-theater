<%= @res %>
<%= @correct %>
<hr>
<div class="row">
  <div class="col-md-12">
    <div class="table-responsive">
      <table class="table table-striped">

        <%= render @header %>

        <tbody>
          <% if !@type.nil? %>
            <%= render partial: 'movie', collection: @result, as: :movie %>
          <% elsif @return_type == "column" %>
            <%= render partial: "movie_column", object: @result %>
          <% elsif @return_type == "array" %>
          <%#= render :partial => 'graph_preview_item', :collection => @available_graphs, :as => :item %>
            <% @result.each do |result| %>
              <%#= render partial: "array_value", value: result %>
              <tr>
                <td>
                  <%= result %>
                </td>
              </tr>
            <% end %>
          <% elsif @return_type == "error" %>
            <%= render partial: "array_value", locals: {value: @result} %>
          <% else %>
            <%= render partial: 'movie', object: @result, as: :movie %>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
<%= @matched_data %>